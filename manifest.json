
{
  "manifest_version": 3,
  "name": "Link Organizer New Tab",
  "version": "1.0",
  "description": "Una nuova scheda personalizzata con link organizzati in colonne e emoji.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
        "resources": ["images/*"],
        "matches": ["<all_urls>"]
    }
]
}
